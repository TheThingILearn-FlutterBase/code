import 'package:supabase_flutter/supabase_flutter.dart';

Future<List<CountriesRow>> fetchFilters(
    String? country, String? continent, String? greater, String? lower) async {
  final query = Supabase.instance.client.from('countries').select('*');
  {
    if (country != null && country.isNotEmpty) query.eq('name', country);
    if (continent != null && continent.isNotEmpty)
      query.neq('continent', continent);
    if (greater != null && greater.isNotEmpty) query.gt('id', greater);
    if (lower != null && lower.isNotEmpty) query.lt('id', lower);
  }
  final response = await query.execute();

  final data = response.data as List<dynamic>;
  final countries = data.map((item) => CountriesRow(item)).toList();
  return countries;
}
